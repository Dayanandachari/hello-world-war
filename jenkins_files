pipeline {
    agent { label "anil" }
    stages {
        stage('checkout') { 
            steps {
              sh "git clone https://github.com/Dayanandachari/hello-world-war"
            }
        }
      stage('build') { 
            steps {
              sh "mvn clean package"
            }
        }  
 stage('deploy') {
            steps {
            
          sh "scp test2.2 ubutnu@172.31.2.28:/home/ubuntu/"
            
       }
 }
}
